@model IEnumerable<websitestintuc.Models.TinTuc>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layoutuser.cshtml";
}

@foreach (var result in Model)
{
    <div class="contentNew">
        <div class="content">
            <div class="topContent">
                <a class="titleNew" href="@result.Link">@result.title</a>
                <span class="descriptionNew">@result.description</span>
            </div>
        </div>
        <div class="image">
            <!-- <img
                src="./pngtree-beautiful-admin-roles-line-vector-icon-png-image_2042211.jpg"
                alt=""
            /> -->
        </div>
    </div>
}